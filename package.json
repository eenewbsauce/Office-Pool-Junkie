{
  "name": "office-pool-junkie",
  "version": "1.0.0",
  "description": "A tool that enables users to automatically pick their teams in a NHL pick 'em pool",
  "main": "index.js",
  "scripts": {
    "start": "node index.js --submitSelections --shouldSaveSelections",
    "populate": "node index.js --shouldSaveMatchups --shouldSaveStats",
    "useSaved": "node index.js --useSavedMatchups --useSavedStats --shouldSaveSelections",
    "backtest": "node backtest --useSavedStats",
    "test": "npm test",
    "postinstall": "mkdir -p ./data/store"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eenewbsauce/Office-Pool-Junkie.git"
  },
  "keywords": [
    "office",
    "pool",
    "junkie"
  ],
  "author": "eenewbsauce",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eenewbsauce/Office-Pool-Junkie/issues"
  },
  "homepage": "https://github.com/eenewbsauce/Office-Pool-Junkie#readme",
  "dependencies": {
    "cheerio": "^0.22.0",
    "minimist": "^1.2.0",
    "ramda": "^0.23.0",
    "request": "^2.79.0"
  }
}
